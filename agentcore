#!/usr/bin/env python3
"""
Development executable for agent-core CLI.
Run this script directly during development: ./agentcore run gpt-4o-mini
"""

import sys
import os

# Add the current directory to Python path
sys.path.insert(0, os.path.dirname(os.path.abspath(__file__)))

from cli.main import main

if __name__ == '__main__':
    main() 